public class ReverseNumber {
    public static void main(String args[]){
        int n=10889;
        while(n>0){
            int LastDigit=n%10;
            System.out.print(LastDigit+"");
            n=n/10;
        }
    }
}
// second approach with diffrent formula 
int rev=0;
int n=10889;
while(n>0){
int lastDigit=n%10;
rev=(rev*10)+lastDigit;
n=n/10;
}
System.out.println(rev);
}
