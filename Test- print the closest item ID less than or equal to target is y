import java.util.Scanner;

class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int nums[]=new int[n];
        for(int i=0; i<n; i++){
            nums[i]=sc.nextInt();
        }
        int target=sc.nextInt();
        int lo=0;
        int hi=n-1;
        int a=0;
        while(lo<=hi){
            int mid=lo+(hi-lo)/2;
            if(nums[mid]==target){
                System.out.println(nums[mid]);
                a++;
                break;
            }
            else if(nums[mid]>target){
                hi=mid-1;
            }
            else{
                lo=mid+1;
            }
        }
        int count=0;
        int min=Integer.MAX_VALUE;
        if(a==0){
            for(int x:nums){
            if(x<target){
                min=Math.min(min,(target-x));
                count++;
            }
        }
        if(count==0){
            System.out.println("NO ELEMENT");
        }
        else{
           System.out.println((target-min));
        }
        }
    }
}
