public class Sollution{
    public static void main(String args[]){
        int arr[]={3,2,4,9,12};
        int n=arr.length;
        int k=3;
      for(int i=0; i<n-k+1; i++){
        for(int j=0; j<k; j++){
            System.out.print(arr[i+j]+" ");
        }
        System.out.println();
      }
    }
}
