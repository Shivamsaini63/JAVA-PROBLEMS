public class Pattern{
    public static void solidRhombus(int n){
        for(int i=1; i<=n; i++){
            //for space
            for(int j=1; j<=(n-i); j++){
                System.out.print(" ");
            }
            //for star
            for(int j=1; j<=n; j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
    public static void main(String args[]){
        solidRhombus(5);
    }
}
