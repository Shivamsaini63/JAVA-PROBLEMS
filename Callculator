import java.util.*;
public class Callculator{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the value of A");
        int A=sc.nextInt();
        System.out.println("Enter the valua of B");
        int B=sc.nextInt();
        System.out.println("Enter the operator");
        char operator=sc.next().charAt(0);
        switch(operator){
            case '+':System.out.println("sum of the numbers" +(A+B));
            break;
            case '-':System.out.println("subtract of the given numvers is=" +(A-B));
            break;
            case '*':System.out.println("multiply of the given numbers is=" +(A*B));
            break;
            case'%': System.out.println("modulo divison of the given numbers is=" +(A%B));
            break;
            case '/':System.out.println("Divison of the given numvers is=" +(A/B));
            break;
            default: System.out.println("wrong operator");
        }

    }
}
