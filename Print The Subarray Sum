public class Sollution{
    public static void main(String args[]){
        int arr[]={3,2,4,9,12};
        int n=arr.length;
        int k=3;
        int sum=0;
      for(int i=0; i<k; i++){
        sum+=arr[i];
      }
      System.out.println(sum);

      for(int j=k; j<n; j++){
        sum=sum+arr[j]-arr[j-k];
        System.out.println(sum);
      }
    }
}
